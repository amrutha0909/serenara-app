{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "^/$",
        "dest": "/dashboard.html",
        "status": 301
      },
      {
        "src": "/api/(.*)",
        "dest": "/api/index.js"
      },
      {
        "src": "/meditate",
        "dest": "/api/index.js"
      },
      {
        "src": "/chatbot",
        "dest": "/api/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
  }